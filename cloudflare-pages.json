{
  "build": {
    "command": "git submodule update --init --recursive || true && pip install -r requirements.txt && echo 'Build complete'",
    "cwd": "."
  },
  "functions": {
    "python": {
      "runtime": "python3.10"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=604800"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}





